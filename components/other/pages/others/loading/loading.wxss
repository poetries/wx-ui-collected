.loading3 {
  width: 20rpx;
  height: 20rpx;
  background: #f6a623;
  border-radius: 50%;
  margin-left: 15rpx;
}

.loading3_1 {
  -webkit-animation: loading3 1s linear infinite alternate;
  animation: loading3 1s linear infinite alternate;
}

.loading3_2 {
  -webkit-animation: loading3 1s linear infinite alternate;
  animation: loading3 1s linear infinite alternate;
  animation-delay: 0.5s;
}

.loading3_3 {
  -webkit-animation: loading3 1s linear infinite alternate;
  animation: loading3 1s linear infinite alternate;
  animation-delay: 1s;
}

@-webkit-keyframes loading3 {
  0% {
    transform: scale(1);
  }

  100% {
    transform: scale(0);
  }
}

@keyframes loading3 {
  0% {
    transform: scale(1);
  }

  100% {
    transform: scale(0);
  }
}

.cube {
  width: 20rpx;
  height: 20rpx;
  background: rgb(244, 128, 73);
  margin-left: -40rpx;
}

.cube1 {
  -webkit-animation: cube_circle 1.5s linear infinite;
  animation: cube_circle 1.5s linear infinite;
}

.cube2 {
  -webkit-animation: cube_circle 1.5s linear infinite;
  animation: cube_circle 1.5s linear infinite;
  animation-delay: 0.75s;
}

@-webkit-keyframes cube_circle {
  25% {
    transform: translate(60rpx, 0);
  }

  50% {
    transform: translate(60rpx, 30rpx);
  }

  75% {
    transform: translate(0, 30rpx);
  }

  100% {
    transform: translateX(0, 0);
  }
}

@keyframes cube_circle {
  25% {
    transform: translate(60rpx, 0);
  }

  50% {
    transform: translate(60rpx, 30rpx);
  }

  75% {
    transform: translate(0, 30rpx);
  }

  100% {
    transform: translateX(0, 0);
  }
}

.pulse {
  width: 8rpx;
  height: 40rpx;
  background: #1ab394;
  margin: 8rpx;
  border-radius: 20rpx;
}

.pulse_1 {
  -webkit-animation: pulse 0.5s linear infinite alternate;
  animation: pulse 0.5s linear infinite alternate;
}

.pulse_2 {
  -webkit-animation: pulse 0.5s linear infinite alternate;
  animation: pulse 0.5s linear infinite alternate;
  animation-delay: 0.1s;
}

.pulse_3 {
  -webkit-animation: pulse 0.5s linear infinite alternate;
  animation: pulse 0.5s linear infinite alternate;
  animation-delay: 0.2s;
}

.pulse_4 {
  -webkit-animation: pulse 0.5s linear infinite alternate;
  animation: pulse 0.5s linear infinite alternate;
  animation-delay: 0.3s;
}

@-webkit-keyframes pulse {
  0% {
    transform: scaleY(1);
  }

  100% {
    transform: scaleY(0);
  }
}

@keyframes pulse {
  0% {
    transform: scaleY(1);
  }

  100% {
    transform: scaleY(0);
  }
}

.line_loading {
  width: 100%;
  height: 2rpx;
  background: #158447;
  opacity: 0.1;
  border-radius: 20rpx;
  -webkit-animation: line 2s ease-in-out infinite;
  animation: line 2s ease-in-out infinite;
}

@-webkit-keyframes line {
  0% {
    transform: translateX(-100%);
  }

  100% {
    transform: translateX(0);
    opacity: 1;
  }
}

@keyframes line {
  0% {
    transform: translateX(-100%);
  }

  100% {
    transform: translateX(0);
    opacity: 1;
  }
}

.knock {
  width: 15rpx;
  height: 15rpx;
  background: #f6a623;
  border-radius: 50%;
}

.knock_first {
  -webkit-animation: knock_first 1s ease-in infinite alternate;
  animation: knock_first 1s ease-in infinite alternate;
}

.knock_last {
  -webkit-animation: knock_last 1s ease-out infinite both alternate;
  animation: knock_last 1s ease-out infinite both alternate;
}

@-webkit-keyframes knock_first {
  0% {
    transform: translate(-30rpx, -10rpx);
  }

  10% {
    transform: translate(-25rpx, -8rpx);
  }

  25% {
    transform: translate(0, 0);
  }

  100% {
    transform: translate(0, 0);
  }
}

@keyframes knock_first {
  0% {
    transform: translate(-30rpx, -10rpx);
  }

  10% {
    transform: translate(-25rpx, -8rpx);
  }

  25% {
    transform: translate(0, 0);
  }

  100% {
    transform: translate(0, 0);
  }
}

@-webkit-keyframes knock_last {
  0% {
    transform: translate(0, 0);
  }

  75% {
    transform: translate(0, 0);
  }

  90% {
    transform: translate(28rpx, -8rpx);
  }

  100% {
    transform: translate(30rpx, -10rpx);
  }
}

@keyframes knock_last {
  0% {
    transform: translate(0, 0);
  }

  75% {
    transform: translate(0, 0);
  }

  90% {
    transform: translate(28rpx, -8rpx);
  }

  100% {
    transform: translate(30rpx, -10rpx);
  }
}

.loading_fade_circle {
  width: 30rpx;
  height: 30rpx;
  background: #1ab394;
  border-radius: 50%;
  -webkit-animation: fading_circle 1s ease-out infinite alternate;
  animation: fading_circle 1s ease-out infinite alternate;
}

@-webkit-keyframes fading_circle {
  0% {
    transform: scale(1.5);
    opacity: 0.2;
  }

  100% {
    transform: scale(0.1);
    opacity: 1;
  }
}

@keyframes fading_circle {
  0% {
    transform: scale(1.5);
    opacity: 0.2;
  }

  100% {
    transform: scale(0.1);
    opacity: 1;
  }
}

.loading_rotate_cube_parent {
  perspective: 20;
  -webkit-perspective: 20;
}

.loading_rotate_cube {
  width: 30rpx;
  height: 30rpx;
  background: #1ab394;
  -webkit-animation: rotate_cube 1s ease-out infinite;
  animation: rotate_cube 1s ease-out infinite;
}

@-webkit-keyframes rotate_cube {
  0% {
    transform: rotateX(180deg);
  }

  50% {
    transform: rotateZ(180deg);
  }

  100% {
    transform: rotateY(180deg);
  }
}

@keyframes rotate_cube {
  0% {
    transform: rotateX(180deg);
  }

  50% {
    transform: rotateZ(180deg);
  }

  100% {
    transform: rotateY(180deg);
  }
}
