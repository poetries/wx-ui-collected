.switch_icon {
  width: 80rpx;
  height: 40rpx;
  display: inline-block;
  position: relative;
}

.switch_1_off {
  border: 1rpx solid #ececec;
  border-radius: 25rpx;
}

.switch_1_on {
  background: #1ab394;
  border-radius: 25rpx;
  border: 1rpx solid #ececec;
}

.switch_1_on_circle, .switch_1_off_circle {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  z-index: 1000;
  top: 0;
  background: #fff;
}

.switch_1_on_circle {
  right: 0;
}

.switch_1_off_circle {
  left: 0;
  border: 1rpx solid #ececec;
}

.switch_content {
  width: 85%;
  float: left;
  line-height: 100rpx;
}

